<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="com.liferay.todolist">
	<namespace>todo</namespace>
	<entity local-service="true" name="Todolist" remote-service="true">

		<!-- PK fields -->
		<column name="taskId" primary="true" type="long"></column>

		<!-- Group instance -->
		<column name="groupId" type="long"></column>

		<!-- Audit fields -->
		<column name="Id" type="long"></column>
		<column name="createdby" type="long"></column>
		<column name="modifiedby" type="String"></column>
		<column name="createDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>

		<!-- Other fields -->
		<column name="enrollmentNo" type="String"></column>
		<column name="firstName" type="String"></column>
		<column name="description" type="String"></column>
		<column name="status" type="String"></column>
		<column name="tags" type="String"></column>

	</entity>
</service-builder>